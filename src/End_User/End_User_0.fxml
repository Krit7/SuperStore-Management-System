<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.effect.SepiaTone?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="End_User.End_User_0_Controller">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="You are a Guest User">
         <font>
            <Font name="System Italic" size="16.0" />
         </font>
      </Label>
      <VBox layoutX="88.0" layoutY="159.0" prefHeight="149.0" prefWidth="201.0">
         <children>
            <Label alignment="CENTER" prefHeight="17.0" prefWidth="204.0" text="Enter item need to be searched" />
            <TextField fx:id="search_field" alignment="CENTER" prefHeight="51.0" prefWidth="201.0" />
            <Button fx:id="search" mnemonicParsing="false" onAction="#Search" prefHeight="79.0" prefWidth="201.0" text="Search" />
         </children>
      </VBox>
      <Button fx:id="browse" layoutX="88.0" layoutY="64.0" mnemonicParsing="false" onAction="#Browse" prefHeight="85.0" prefWidth="201.0" text="Browse" />
      <Button fx:id="checkout" layoutX="88.0" layoutY="312.0" mnemonicParsing="false" onAction="#Checkout" prefHeight="66.0" prefWidth="201.0" text="Checkout Cart" />
      <Button fx:id="logout" layoutX="315.0" layoutY="14.0" mnemonicParsing="false" onAction="#logout" text="LOGOUT" />
   </children>
   <effect>
      <SepiaTone />
   </effect>
</AnchorPane>

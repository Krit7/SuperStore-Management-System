            <Button fx:id="manage" layoutX="197.0" layoutY="51.0" mnemonicParsing="false" onAction="#Manage_Cost_demand" prefHeight="25.0" prefWidth="193.0" text="Manage cost / demand" GridPane.columnIndex="1" />
         </children>
      </GridPane>
      <Button fx:id="logout" layoutX="323.0" layoutY="27.0" mnemonicParsing="false" onAction="#logout" text="LOGOUT" />
      <Button fx:id="add_product1" alignment="CENTER" contentDisplay="CENTER" layoutX="107.0" layoutY="119.0" mnemonicParsing="false" onAction="#Add_Product" prefHeight="25.0" prefWidth="186.0" text="Add product" />
   </children>
   <effect>
      <InnerShadow choke="1.0" height="30.0" radius="14.5" width="30.0" />
   </effect>
</AnchorPane>

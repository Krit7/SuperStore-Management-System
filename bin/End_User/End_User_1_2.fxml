<AnchorPane prefHeight="400.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="End_User.End_User_1_2_Controller">
   <children>
      <Label layoutX="14.0" layoutY="14.0" text="You are a Guest User">
         <font>
            <Font name="System Italic" size="16.0" />
         </font>
      </Label>
      <Label layoutX="50.0" layoutY="39.0" prefHeight="56.0" prefWidth="156.0" text="Products">
         <font>
            <Font size="39.0" />
         </font>
      </Label>
      <ListView fx:id="List" layoutX="25.0" layoutY="110.0" prefHeight="233.0" prefWidth="349.0" />
      <Button fx:id="SbyN" layoutX="25.0" layoutY="360.0" mnemonicParsing="false" onAction="#Name_Sort" prefHeight="25.0" prefWidth="92.0" text="Sort by name" />
      <Button fx:id="add_cart" layoutX="265.0" layoutY="67.0" mnemonicParsing="false" onAction="#Add_Cart" prefHeight="39.0" prefWidth="109.0" text="Add to Cart" />
